<h1>New audiofile</h1>

<% form_for(@audiofile) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :file %><br />
    <%= f.text_field :file %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', audiofiles_path %>
