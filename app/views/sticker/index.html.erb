<h1>Sticker List </h1>
<%= link_to 'new', :action => 'new'%>

<ul>
<li> Title: <%= @title %>
<li> Link: <%= url_for :controller => 'caststudio', :action => 'rpc', :only_path => false %>
<li> Description: <%= @description %>
<li> Language: <%= @language %>
<li> PubDate: <%= @pubdate %>
<li> Category: <%= @ch_category %>
</ul>

<table class="style_table" cellspacing="1" border="0">
<tr><th class="style_th">title</th>
<th class="style_th">desc</th>
<th class="style_th">date</th>
</tr>
<% @items.each do |i| -%>
<tr>
<td class="style_td"> 
<%= link_to i.title, :action => 'show', :id => i.id %>
</td>
<td class="style_td"> <%= i.desc %></td>
<td class="style_td"> <%= i.pubdate %></td>
</tr>
<% end -%>
</table>
